    "url": "https://github.com/egomadking/egomadking.github.io/issues"
  },
  "homepage": "https://github.com/egomadking/egomadking.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  }
